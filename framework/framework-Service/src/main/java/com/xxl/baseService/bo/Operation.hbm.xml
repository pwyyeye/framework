<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "hibernate-mapping-3.0.dtd">



  <hibernate-mapping>
     <class name="com.xxl.baseService.bo.Operation"  table="RB_OPERATIONS">
        <id name="code" type="String" >
        <column name="OPERATIONID" sql-type="varchar(10)" not-null="true"/>
        <generator class="uuid.hex"/>
        </id>
        <property name="name">
        <column name="OPERATIONNAME" sql-type="varchar(20)" />
        </property>
     </class>
  </hibernate-mapping>
