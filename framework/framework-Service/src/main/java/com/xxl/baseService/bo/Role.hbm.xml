<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "hibernate-mapping-3.0.dtd">
  <hibernate-mapping>
     <class name="com.xxl.baseService.bo.Role" table="RB_ROLES">
        <id name="id" column="ROLEID">
         <generator class="identity"/>
        </id>
        <property name="rolename">
        <column name="ROLENAME" sql-type="varchar(100)" />
        </property>
        <property name="description">
        <column name="DESCRIPTION" sql-type="varchar(100)" />
        </property>
         <property name="valid">
        <column name="VALID" sql-type="int" />
        </property>
       <many-to-one name="module" class="common.businessObject.ItModule" column="SYSTEMID"/>
       <many-to-one name="organise" class="common.os.bo.SyOrganise" column="OR_ID"/>

     </class>
  </hibernate-mapping>
